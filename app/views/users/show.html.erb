<div class="container text-center">
<h2>Name: <%= @user.name %><br>
<h3>Age: <%= @user.age%></h3>
<h3><%= "#{@user.name}" %>'s Bio</h3>
<p><%= @user.bio %></p>
<%= link_to 'Edit Profile', edit_user_path(@user.id) %> <br><br>
<%= button_to 'Delete Profile', user_path(@user), method: :delete %>
</div>

